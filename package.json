{
  "private": false,
  "name": "@axieinfinity/r3f-axie-starter",
  "version": "0.0.1",
  "description": "Pomodoro, Buba, Puffy",
  "main": "build/index.js",
  "files": [
    "build/**/*"
  ],
  "types": "build/index.d.ts",
  "scripts": {
    "build:clean": "rimraf build",
    "build:rollup": "rollup -c",
    "build": "npm-run-all build:clean build:rollup",
    "lint:ts": "eslint ./packages --ext .ts,.tsx --config .eslintrc --fix",
    "lint:styles": "stylelint \"**/*.css\" --fix",
    "lint": "npm-run-all lint:ts lint:styles",
    "format": "prettier --write .",
    "semantic-release": "semantic-release",
    "release": "npm run build && npm publish",
    "dev": "npm run release && cd demo && npm start",
    "prepare": "husky install"
  },
  "keywords": [
    "@react-three/fiber",
    "@react-three/drei",
    "@react-three/rapier",
    "three",
    "axieinfinity"
  ],
  "author": "smsonhaaa",
  "peerDependencies": {
    "@react-three/fiber": "8.9.1",
    "react": "18.2.0",
    "three": ">=0.139.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.7.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@react-three/drei": "^9.83.9",
    "@react-three/fiber": "^8.14.1",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/react-dom": "^18.2.7",
    "@types/three": "^0.156.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.1",
    "rollup-plugin-ignore": "^1.0.10",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "stylelint": "^15.10.3",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^4.0.2",
    "three": "^0.156.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
