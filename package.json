{
  "name": "@sms0nhaaa/r3f-axie-starter",
  "version": "0.0.1",
  "description": "Axie Starter: Buba, Pomodoro, and Puffy",
  "keywords": [
    "react-three-fiber",
    "axie-starter",
    "axieinfinity"
  ],
  "maintainers": [
    "Son Ha Nguyen (https://github.com/sms0nhaaa)"
  ],
  "homepage": "https://github.com/axieinfinity/r3f-axie-starter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axieinfinity/r3f-axie-starter.git"
  },
  "bugs": {
    "url": "https://github.com/axieinfinity/r3f-axie-starter/issues"
  },
  "files": [
    "build/**/*"
  ],
  "main": "build/index.cjs.js",
  "module": "build/index.js",
  "types": "build/index.d.ts",
  "sideEffects": false,
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        0
      ]
    }
  },
  "scripts": {
    "prebuild": "rimraf build && npm run typegen",
    "build": "rollup -c",
    "prepare": "npm run build && husky install",
    "eslint:fix": "eslint --fix .",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "format": "npm run prettier:fix && npm run eslint:fix",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false --strict --jsx react",
    "typegen": "tsc --emitDeclarationOnly",
    "release": "semantic-release"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@react-spring/three": "~9.6.1",
    "@react-three/drei": "^9.83.9",
    "three-stdlib": "^2.25.1",
    "zustand": "^3.5.13"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-transform-modules-commonjs": "^7.14.0",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@react-three/fiber": "^8.0.8",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "@types/three": "^0.149.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^14.0.1",
    "prettier": "^2.4.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.78.1",
    "rollup-plugin-glslify": "^1.3.0",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^21.0.6",
    "three": "^0.149.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vite": "^4.3.6",
    "vite-plugin-glslify": "^2.0.1",
    "yarn": "^1.22.17"
  },
  "peerDependencies": {
    "@react-three/fiber": ">=8.0",
    "react": ">=18.0",
    "react-dom": ">=18.0",
    "three": ">=0.137"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
